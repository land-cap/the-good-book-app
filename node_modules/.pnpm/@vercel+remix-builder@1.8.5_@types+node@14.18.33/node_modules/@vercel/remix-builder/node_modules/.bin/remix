#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/remix-run-dev/dist/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/remix-run-dev/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/remix-run-dev/dist/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/remix-run-dev/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules:/Users/land_cap/projects/the-good-book-app/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../../@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/remix-run-dev/dist/cli.js" "$@"
else
  exec node  "$basedir/../../../../../../@vercel+remix-run-dev@1.15.0_@types+node@14.18.33/node_modules/@vercel/remix-run-dev/dist/cli.js" "$@"
fi
